@{
    ViewBag.Title = "Home Page";
}

<div class="row">
    <div class="col-md-4">
        <h2>Getting started</h2>
        <p>
            <input type="text" class="form-control" id="txtkeyword" placeholder="buscar" />
        </p>
        <p><a class="btn btn-default" href="javascript:void(0)" onclick="btnbuscar()">buscar</a></p>
    </div>
    <div class="col-md-4">
        <div class="container" id="ListYoutube">

        </div>
    </div>
</div>

<script>
    function btnbuscar()
    {
        $.ajax(
            {
                url: '@Url.Action("BuscarLista")?keyword='+$('#txtkeyword').val(),
                type: 'GET',
                success: function (result)
                {
                    $('#ListYoutube').html(result);
                    //respuesta
                }
            });
    }
</script>